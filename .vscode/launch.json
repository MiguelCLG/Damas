  "version": "0.2.0",
  "configurations": [
    // For these launch configurations to work, you need to setup a GODOT4
    // environment variable. On mac or linux, this can be done by adding
    // the following to your .zshrc, .bashrc, or .bash_profile file:
    // export GODOT4="/Applications/Godot.app/Contents/MacOS/Godot"
    {
      "name": "Play",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "D:\\PC FOLDERS/Desktop/Godot_v3.6-stable_mono_win64/Godot_v3.6-stable_mono_win64.exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
    },
    {
      "name": "Play (Select Scene)",
      "type": "godot-mono",
      "request": "launch",
      "preLaunchTask": "build",
      "mode": "executable",
      "executable": "D:\\PC FOLDERS/Desktop/Godot_v3.6-stable_mono_win64/Godot_v3.6-stable_mono_win64.exe",
      "executableArguments": [
        "--path",
        "${workspaceFolder}",
        "${command:SelectLaunchScene}"
      ],
    },
    /*  {
      "name": "Play",
      "type": "coreclr",
      "request": "launch",
      "logging": {"engineLogging": true},
      "preLaunchTask": "build",
      "program": "D:\\PC FOLDERS/Desktop/Godot_v4.1-stable_mono_win64/Godot_v4.1-stable_mono_win64.exe",
      "args": ["--path", "${workspaceFolder}"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
    }, */
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "D:/PC FOLDERS/Desktop/Godot_v4.3-stable_mono_win64/Godot_v4.3-stable_mono_win64.exe",
      "args": [
        "--editor",
        "--path",
        "${workspaceFolder}"
      ],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Launch",
      "type": "godot-mono",
      "request": "launch",
      "mode": "executable",
      "preLaunchTask": "build",
      "executable": "D:\\PC FOLDERS/Desktop/Godot_v4.3-stable_mono_win64/Godot_v4.3-stable_mono_win64.exe",
      "executableArguments": [
        "--path",
        "${workspaceFolder}"
      ]
    },
    {
      "name": "Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
